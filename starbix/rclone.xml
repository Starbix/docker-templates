<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Date>2016-15-08</Date>
  <Name>rclone</Name>
  <Description>
  "rsync for cloud storage" - Google Drive, Amazon Drive, S3, Dropbox, Backblaze B2, One Drive, Swift, Hubic, Cloudfiles, Google Cloud Storage, Yandex Files  </Description>
  <Project>https://rclone.org</Project>
  <Overview>This is a Docker container for running FileBot, a media file organizer. Drop files into the input directory, and they'll be cleaned up and moved to the output directory.</Overview>
  <Registry>https://hub.docker.com/r/starbix/rclone/</Registry>
  <Repository>starbix/rclone</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
  </Environment>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>

  <Banner>https://raw.githubusercontent.com/starbix/docker-templates/master/starbix/img/rclone-banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/starbix/docker-templates/master/starbix/img/rclone-icon.png</Icon>
  <ExtraParams></ExtraParams>
<BaseImage>alpine:latest</BaseImage>
</Container>
