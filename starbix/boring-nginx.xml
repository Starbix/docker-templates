<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category></Category>
  <Date>2016-12-24</Date>
  <Name>boring-nginx</Name>
  <Description></Description>
  <Project>https://nginx.com/</Project>
  <Registry>https://hub.docker.com/aptalce/nginx-letsencrypt/</Registry>
  <Repository>wonderfall/boring-nginx</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>UID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>GID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8000</HostPort>
        <ContainerPort>8000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>4430</HostPort>
        <ContainerPort>4430</ContainerPort>
        <Protocol>utcp</Protocol>
      </Port>
     </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/sites-enabled</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/www</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/conf.d</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/passwds</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/certs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/log/nginx</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
<WebUI>https://[IP]:[PORT:8000]</WebUI>
  <Banner>https://raw.githubusercontent.com/starbix/docker-templates/master/starbix/img/nginx-banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/starbix/docker-templates/master/starbix/img/nginx-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
